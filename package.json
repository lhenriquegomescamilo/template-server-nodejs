{
  "name": "typescript-course-to-the-sky",
  "version": "1.0.0",
  "description": "API desenvolvida, com TS, ES6, SEQUELIZE",
  "main": "server/server.ts",
  "scripts": {
    "compile": "./node_modules/typescript/bin/tsc -p tsconfig.json",
    "start-dev": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "start": "./node_modules/typescript/bin/tsc -p tsconfig.json && NODE_ENV=development node ./build/server/server.js",
    "unit-test": "NODE_ENV=development mocha --opts build/test/unit/config/mocha.opts ./build/test/unit/unit.test.js ",
    "integration-test": " npm run compile && NODE_ENV=development mocha --opts build/test/integration/config/mocha.opts ./build/test/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html  npm run compile && npm run unit-test",
    "test": "npm run compile && npm run integration-test"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "ES6"
  ],
  "author": "Luis Henrique Gomes CAmilo",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.65",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "http": "0.0.0",
    "http-status": "^1.0.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "mysql2": "^1.3.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.2.3",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^2.7.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/body-parser": "^1.16.3",
    "@types/chai": "^4.0.0",
    "@types/express": "^4.0.35",
    "@types/http-status": "^0.2.29",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/pg": "^6.1.40",
    "@types/sequelize": "^4.0.55",
    "@types/supertest": "^2.0.0",
    "chai": "^4.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "sequelize": "^5.21.6",
    "supertest": "^3.0.0",
    "testdouble": "^2.1.2",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.4"
  }
}
